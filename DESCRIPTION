Package: migest
Type: Package
Title: Methods for the Indirect Estimation of Bilateral Migration
Version: 2.0.1
Authors@R: c(person(c("Guy", "J."), "Abel", role = c("aut", "cre"),
                    email = "g.j.abel@gmail.com",
